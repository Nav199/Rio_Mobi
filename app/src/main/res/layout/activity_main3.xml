        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.648" />

    <EditText
        android:id="@+id/editTextText8"
        android:layout_width="188dp"
        android:layout_height="39dp"
        android:ems="10"
